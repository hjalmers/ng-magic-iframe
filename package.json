{
  "name": "ng-magic-iframe-demo",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --base-href /ng-magic-iframe/ --prod",
    "prebuild": "rimraf dist/ng-magic-iframe-demo && replace __TRAVIS_BUILD_NUMBER__ $TRAVIS_BUILD_NUMBER src/environments/environment.ts && replace __TRAVIS_BUILD_NUMBER__ $TRAVIS_BUILD_NUMBER src/environments/environment.prod.ts",
    "build-lib": "ng build ng-magic-iframe && copyfiles -f CHANGELOG.md LICENSE.md README.md dist/ng-magic-iframe",
    "prebuild-lib": "rimraf dist/ng-magic-iframe && npm --prefix ./projects/ng-magic-iframe install ./projects/ng-magic-iframe",
    "build-element": "npm run build-element-es2015 && npm run build-element-es5",
    "build-element-new": "ng build magic-iframe --prod --extraWebpackConfig ../../webpack.extra.js --output-hashing none --single-bundle true\n",
    "postbuild-element": "rimraf dist/magic-iframe/{main,polyfills,runtime,scripts}*.js && gzip dist/magic-iframe/*.js && copyfiles -f dist/magic-iframe/* projects/magic-iframe/src/*.html dist/ng-magic-iframe-demo/web-component/",
    "build-element-es2015": "ng build magic-iframe --prod --output-hashing=none",
    "postbuild-element-es2015": "concat ./dist/magic-iframe/runtime.js ./dist/magic-iframe/main.js -o ./dist/magic-iframe/magic-iframe.js",
    "build-element-es5": "ng build magic-iframe --prod --output-hashing=none --delete-output-path false --ts-config ./projects/magic-iframe/tsconfig.app-es5.json",
    "postbuild-element-es5": "concat ./dist/magic-iframe/runtime.js ./dist/magic-iframe/polyfills.js ./dist/magic-iframe/main.js -o ./dist/magic-iframe/magic-iframe-es5.js",
    "serve-demo": "ws -d ./dist/ng-magic-iframe-demo/ -p 8000 -z -r \"/ng-magic-iframe/* -> http://localhost:8000/$1\"",
    "bundle-report": "webpack-bundle-analyzer dist/magic-iframe/stats.json",
    "copy-bundles": "node copy-bundles.js",
    "test": "ng test",
    "lint": "ng lint",
    "lint-lib": "ng lint ng-magic-iframe",
    "e2e": "ng e2e",
    "commit": "git-cz",
    "travis-deploy-once": "travis-deploy-once --pro",
    "semantic-release": "semantic-release && npm run build"
  },
  "release": {
    "pkgRoot": "dist/ng-magic-iframe"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.1",
    "@angular/common": "^8.0.1",
    "@angular/compiler": "^8.0.1",
    "@angular/core": "^8.0.1",
    "@angular/elements": "^8.0.1",
    "@angular/forms": "^8.0.1",
    "@angular/http": "^7.2.11",
    "@angular/platform-browser": "^8.0.1",
    "@angular/platform-browser-dynamic": "^8.0.1",
    "@angular/router": "^8.0.1",
    "@ng-bootstrap/ng-bootstrap": "^4.2.1",
    "@sebgroup/bootstrap": "^3.2.0",
    "@webcomponents/custom-elements": "^1.2.2",
    "@webcomponents/webcomponentsjs": "^2.2.7",
    "core-js": "^2.6.5",
    "document-register-element": "^1.7.2",
    "ngx-build-plus": "^7.7.6",
    "prismjs": "^1.16.0",
    "rxjs": "~6.5.2",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.800.3",
    "@angular-devkit/build-ng-packagr": "^0.800.3",
    "@angular/cli": "^8.0.3",
    "@angular/compiler-cli": "^8.0.1",
    "@angular/language-service": "^8.0.1",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^8.10.45",
    "codelyzer": "^5.1.0",
    "concat": "^1.0.3",
    "copy": "^0.3.2",
    "copyfiles": "^2.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "gzip-cli": "^0.1.3",
    "husky": "^1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "local-web-server": "^2.6.1",
    "ng-packagr": "^5.3.0",
    "protractor": "^5.4.2",
    "replace": "^1.1.0",
    "semantic-release": "^15.13.3",
    "travis-deploy-once": "^5.0.11",
    "ts-node": "~7.0.0",
    "tslib": "^1.10.0",
    "tslint": "~5.11.0",
    "typescript": "^3.4.5",
    "webpack-bundle-analyzer": "^3.1.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run lint-lib",
      "commit-msg": "commitlint --edit"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
